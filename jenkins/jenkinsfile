pipeline {
   agent {
    label 'devops'
   }
  
   stages {
    stage("App Code Checkout") {
      steps {
        sh "git branch: 'master', url: 'https://github.com/prateekkumawat/jenkins-mansi-project.git' "
      }
    }
    
    stage("Build Docker Image"){
      steps {
        sh " docker build -t sampleapp:latest . "
      }
    }
   }
}